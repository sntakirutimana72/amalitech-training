<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.lab3.bankmanagementsystem.gui.WithdrawController">
  <VBox layoutX="3.0" layoutY="1.0" prefHeight="268.0" prefWidth="347.0" style="-fx-background-color: #f1f1f1;"
        AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
        AnchorPane.topAnchor="0.0">
    <padding>
      <Insets bottom="50.0" left="20.0" right="20.0" top="10.0"/>
    </padding>
    <Label prefHeight="44.0" prefWidth="600.0" text="Withdrawal Slip">
      <font>
        <Font size="33.0"/>
      </font>
      <VBox.margin>
        <Insets bottom="20.0"/>
      </VBox.margin>
      <padding>
        <Insets bottom="10.0" top="10.0"/>
      </padding>
    </Label>
    <VBox prefHeight="50.0">
      <VBox.margin>
        <Insets bottom="15.0"/>
      </VBox.margin>
      <AnchorPane prefHeight="200.0" prefWidth="200.0">
        <Label layoutX="85.0" layoutY="1.0" text="Account Number" AnchorPane.bottomAnchor="0.0"
               AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
      </AnchorPane>
      <TextField fx:id="accountNumberField" promptText="Enter account number"/>
    </VBox>
    <VBox layoutX="30.0" layoutY="99.0" prefHeight="50.0">
      <VBox.margin>
        <Insets bottom="15.0"/>
      </VBox.margin>
      <AnchorPane prefHeight="200.0" prefWidth="200.0">
        <Label layoutX="85.0" layoutY="1.0" text="Amount" AnchorPane.bottomAnchor="0.0"
               AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
      </AnchorPane>
      <TextField fx:id="amountField" promptText="Enter amount"/>
    </VBox>
    <AnchorPane prefHeight="30.0" prefWidth="200.0">
      <VBox.margin>
        <Insets top="10.0"/>
      </VBox.margin>
      <Button layoutX="181.0" layoutY="1.0" mnemonicParsing="false" text="Process Transaction"
              AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
              AnchorPane.topAnchor="0.0" onAction="#onWithdraw"/>
    </AnchorPane>
  </VBox>
</AnchorPane>
