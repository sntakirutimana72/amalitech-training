<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            fx:id="mainLayout" styleClass="bg-primary" prefHeight="400" prefWidth="700"
            fx:controller="com.lab3.bankmanagementsystem.gui.DashboardController"
            xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml">

   <left>
     <VBox prefHeight="458.0" prefWidth="167.0" BorderPane.alignment="CENTER">
       <padding>
         <Insets bottom="10.0" left="10.0" right="10.0" top="30.0"/>
       </padding>
       <Button mnemonicParsing="false" prefHeight="29.0" prefWidth="169.0" text="HOME"
               onAction="#switchToHome">
         <VBox.margin>
           <Insets bottom="10.0"/>
         </VBox.margin>
       </Button>
       <Button mnemonicParsing="false" prefHeight="29.0" prefWidth="178.0" text="NEW ACCOUNT"
               onAction="#switchToOpenAccount">
         <VBox.margin>
           <Insets bottom="10.0"/>
         </VBox.margin>
       </Button>
       <Button mnemonicParsing="false" prefHeight="29.0" prefWidth="187.0" text="DEPOSIT"
               onAction="#switchToDeposit">
         <VBox.margin>
           <Insets bottom="10.0"/>
         </VBox.margin>
       </Button>
       <Button mnemonicParsing="false" prefHeight="29.0" prefWidth="174.0" text="WITHDRAW"
               onAction="#switchToWithdraw">
         <VBox.margin>
           <Insets bottom="10.0"/>
         </VBox.margin>
       </Button>
       <Button mnemonicParsing="false" prefHeight="29.0" prefWidth="164.0" text="CHECK BALANCE"
               onAction="#switchToCheckBalance"/>
     </VBox>
   </left>
</BorderPane>
